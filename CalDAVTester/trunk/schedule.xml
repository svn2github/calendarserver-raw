	<test-suite name='SCHEDULE Errors' ignore='no'>
		<test name='1' ignore='no'>
			<description>SCHEDULE on invalid outbox</description>
			<request>
				<method>SCHEDULE</method>
				<header>
					<name>Organizer</name>
					<value>mailto:user01@example.com</value>
				</header>
				<header>
					<name>Recipient</name>
					<value>mailto:user02@example.com, mailto:user03@example.com</value>
				</header>
				<ruri>$pathprefix:/inbox/</ruri>
				<data>
					<content-type>text/xml; charset=utf-8</content-type>
					<filepath>Resource/schedule/3.txt</filepath>
				</data>
				<verify>
					<callback>prepostcondition</callback>
					<arg>
						<name>error</name>
						<value>urn:ietf:params:xml:ns:caldavsupported-collection</value>
					</arg>
				</verify>
			</request>
		</test>
		<test name='2' ignore='no'>
